
public class ProductValidator {
	
	static {
		System.out.println("Static y apýcý blok çalýþtý.");
	}
	
	public ProductValidator() {
		System.out.println("Yapýcý blok new'lenmeden çalýþmaz.");
	}
	public static boolean isValid(Product product) {
		if (product.price>0 && !product.name.isEmpty()) {
			return true;
		}else {
			return false;
		}
	}
	
	public void another() {
		
	}
}
